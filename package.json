{
  "name": "commandline-extention",
  "displayName": "Command & Prompt Tracker",
  "publisher": "skdsam",
  "description": "Manage reusable commands and prompts with a premium native UI.",
  "version": "1.2.3",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/skdsam/commandline-extention.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "command-tracker-sidebar",
          "title": "Command Tracker",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "command-tracker-sidebar": [
        {
          "type": "webview",
          "id": "command-tracker-view",
          "name": "Commands & Prompts",
          "icon": "media/icon.png"
        }
      ]
    },
    "commands": [
      {
        "command": "command-tracker.addEntry",
        "title": "Add to Command Tracker"
      },
      {
        "command": "command-tracker.sync",
        "title": "Sync Commands (Git)"
      },
      {
        "command": "command-tracker.resetGit",
        "title": "Command Tracker: Reset Git Configuration"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.44"
  }
}
